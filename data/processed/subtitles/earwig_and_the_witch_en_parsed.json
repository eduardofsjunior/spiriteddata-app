  "subtitles": [
    {
      "subtitle_index": 1,
      "start_time": 1444.075,
      "end_time": 1445.2,
      "duration": 1.125,
      "dialogue_text": "Scellinc."
    },
    {
      "subtitle_index": 2,
      "start_time": 1445.325,
      "end_time": 1446.575,
      "duration": 1.25,
      "dialogue_text": "Sir."
    },
    {
      "subtitle_index": 3,
      "start_time": 1446.825,
      "end_time": 1447.95,
      "duration": 1.125,
      "dialogue_text": "How is the girl?"
    },
    {
      "subtitle_index": 4,
      "start_time": 1448.95,
      "end_time": 1450.95,
      "duration": 2.0,
      "dialogue_text": "Everything is well, sir."
    },
    {
      "subtitle_index": 5,
      "start_time": 1452.2,
      "end_time": 1456.075,
      "duration": 3.875,
      "dialogue_text": "She sleeps soundly. Her appetite is good. All is as it should be."
    },
    {
      "subtitle_index": 6,
      "start_time": 1458.45,
      "end_time": 1459.575,
      "duration": 1.125,
      "dialogue_text": "And the teeth?"
    },
    {
      "subtitle_index": 7,
      "start_time": 1460.575,
      "end_time": 1462.2,
      "duration": 1.625,
      "dialogue_text": "Everything is well, sir."
    },
    {
      "subtitle_index": 8,
      "start_time": 1463.825,
      "end_time": 1466.325,
      "duration": 2.5,
      "dialogue_text": "The month's remittance has been sent."
    },
    {
      "subtitle_index": 9,
      "start_time": 1467.075,
      "end_time": 1468.699,
      "duration": 1.6240000000000236,
      "dialogue_text": "Do you require anything else?"
    },
    {
      "subtitle_index": 10,
      "start_time": 1468.7,
      "end_time": 1469.95,
      "duration": 1.25,
      "dialogue_text": "No, sir."
    },
    {
      "subtitle_index": 11,
      "start_time": 1471.825,
      "end_time": 1473.825,
      "duration": 2.0,
      "dialogue_text": "There will be no further payments."
    },
    {
      "subtitle_index": 12,
      "start_time": 1475.7,
      "end_time": 1476.825,
      "duration": 1.125,
      "dialogue_text": "Sir?"
    },
    {
      "subtitle_index": 13,
      "start_time": 1477.825,
      "end_time": 1480.325,
      "duration": 2.5,
      "dialogue_text": "You must start preparing the girl to leave."
    },
    {
      "subtitle_index": 14,
      "start_time": 1481.7,
      "end_time": 1482.825,
      "duration": 1.125,
      "dialogue_text": "Leave?"
    },
    {
      "subtitle_index": 15,
      "start_time": 1486.575,
      "end_time": 1492.575,
      "duration": 6.0,
      "dialogue_text": "You will bring her to us on the 6th of next month, in 13 days' time."
    },
    {
      "subtitle_index": 16,
      "start_time": 1495.325,
      "end_time": 1499.45,
      "duration": 4.125,
      "dialogue_text": "In the meantime, you must teach her how to behave outside."
    },
    {
      "subtitle_index": 17,
      "start_time": 1500.325,
      "end_time": 1501.95,
      "duration": 1.625,
      "dialogue_text": "Do you understand, Scellinc?"
    },
    {
      "subtitle_index": 18,
      "start_time": 1502.95,
      "end_time": 1504.325,
      "duration": 1.375,
      "dialogue_text": "Yes, sir."
    },
    {
      "subtitle_index": 19,
      "start_time": 1752.7,
      "end_time": 1754.7,
      "duration": 2.0,
      "dialogue_text": "You must get used to being outside."
    },
    {
      "subtitle_index": 20,
      "start_time": 1984.95,
      "end_time": 1986.075,
      "duration": 1.125,
      "dialogue_text": "Mia!"
    },
    {
      "subtitle_index": 21,
      "start_time": 1994.575,
      "end_time": 1995.7,
      "duration": 1.125,
      "dialogue_text": "Mia!"
    },
    {
      "subtitle_index": 22,
      "start_time": 2040.7,
      "end_time": 2041.825,
      "duration": 1.125,
      "dialogue_text": "Mia!"
    },
    {
      "subtitle_index": 23,
      "start_time": 2266.075,
      "end_time": 2267.2,
      "duration": 1.125,
      "dialogue_text": "Stop it!"
    },
    {
      "subtitle_index": 24,
      "start_time": 2277.075,
      "end_time": 2278.2,
      "duration": 1.125,
      "dialogue_text": "Stop!"
    },
    {
      "subtitle_index": 25,
      "start_time": 2560.7,
      "end_time": 2562.074,
      "duration": 1.374000000000251,
      "dialogue_text": "What can I get you?"
    },
    {
      "subtitle_index": 26,
      "start_time": 2562.075,
      "end_time": 2563.2,
      "duration": 1.125,
      "dialogue_text": "A Tripel."
    },
    {
      "subtitle_index": 27,
      "start_time": 2610.325,
      "end_time": 2611.575,
      "duration": 1.25,
      "dialogue_text": "May I?"
    },
    {
      "subtitle_index": 28,
      "start_time": 2630.075,
      "end_time": 2631.2,
      "duration": 1.125,
      "dialogue_text": "I am a traveller, sir."
    },
    {
      "subtitle_index": 29,
      "start_time": 2633.575,
      "end_time": 2635.2,
      "duration": 1.625,
      "dialogue_text": "A stranger to this city."
    },
    {
      "subtitle_index": 30,
      "start_time": 2637.95,
      "end_time": 2639.075,
      "duration": 1.125,
      "dialogue_text": "I live far away…"
    },
    {
      "subtitle_index": 31,
      "start_time": 2641.45,
      "end_time": 2643.7,
      "duration": 2.25,
      "dialogue_text": "in the shadow of a great cathedral."
    },
    {
      "subtitle_index": 32,
      "start_time": 2653.325,
      "end_time": 2654.45,
      "duration": 1.125,
      "dialogue_text": "No matter."
    },
    {
      "subtitle_index": 33,
      "start_time": 2658.2,
      "end_time": 2659.95,
      "duration": 1.75,
      "dialogue_text": "I think I shall join you."
    },
    {
      "subtitle_index": 34,
      "start_time": 2682.825,
      "end_time": 2683.95,
      "duration": 1.125,
      "dialogue_text": "Thank you."
    },
    {
      "subtitle_index": 35,
      "start_time": 2702.95,
      "end_time": 2706.95,
      "duration": 4.0,
      "dialogue_text": "And now, sir… What is your name?"
    },
    {
      "subtitle_index": 36,
      "start_time": 2711.825,
      "end_time": 2713.45,
      "duration": 1.625,
      "dialogue_text": "Albert Scellinc."
    },
    {
      "subtitle_index": 37,
      "start_time": 2717.95,
      "end_time": 2719.075,
      "duration": 1.125,
      "dialogue_text": "Albert…"
    },
    {
      "subtitle_index": 38,
      "start_time": 2721.95,
      "end_time": 2725.7,
      "duration": 3.75,
      "dialogue_text": "We’ve met before... I am sure of it."
    },
    {
      "subtitle_index": 39,
      "start_time": 2728.2,
      "end_time": 2733.325,
      "duration": 5.125,
      "dialogue_text": "Although for the moment, I can’t quite recall where…"
    },
    {
      "subtitle_index": 40,
      "start_time": 2734.95,
      "end_time": 2737.325,
      "duration": 2.375,
      "dialogue_text": "On the field of conflict, perhaps?"
    },
    {
      "subtitle_index": 41,
      "start_time": 2741.2,
      "end_time": 2745.2,
      "duration": 4.0,
      "dialogue_text": "Covered in mud, blood and shit."
    },
    {
      "subtitle_index": 42,
      "start_time": 2754.7,
      "end_time": 2760.2,
      "duration": 5.5,
      "dialogue_text": "Or was it long before… at the orphanage?"
    },
    {
      "subtitle_index": 43,
      "start_time": 2769.825,
      "end_time": 2770.95,
      "duration": 1.125,
      "dialogue_text": "No…"
    },
    {
      "subtitle_index": 44,
      "start_time": 2772.575,
      "end_time": 2779.325,
      "duration": 6.75,
      "dialogue_text": "No, in fact, I believe it was after the war… when you had a wife."
    },
    {
      "subtitle_index": 45,
      "start_time": 2781.825,
      "end_time": 2783.075,
      "duration": 1.25,
      "dialogue_text": "Wife?"
    },
    {
      "subtitle_index": 46,
      "start_time": 2785.2,
      "end_time": 2788.075,
      "duration": 2.875,
      "dialogue_text": "Didn’t you take a wife after the war?"
    },
    {
      "subtitle_index": 47,
      "start_time": 2793.2,
      "end_time": 2794.575,
      "duration": 1.375,
      "dialogue_text": "I don’t have a wife!"
    },
    {
      "subtitle_index": 48,
      "start_time": 2797.075,
      "end_time": 2801.7,
      "duration": 4.625,
      "dialogue_text": "Marie is dead, it's true."
    },
    {
      "subtitle_index": 49,
      "start_time": 2806.325,
      "end_time": 2808.075,
      "duration": 1.75,
      "dialogue_text": "But then there's the other one."
    },
    {
      "subtitle_index": 50,
      "start_time": 2815.575,
      "end_time": 2816.95,
      "duration": 1.375,
      "dialogue_text": "She is waking…"
    },
    {
      "subtitle_index": 51,
      "start_time": 2820.2,
      "end_time": 2821.7,
      "duration": 1.5,
      "dialogue_text": "I don't have a wife!"
    },
    {
      "subtitle_index": 52,
      "start_time": 2830.7,
      "end_time": 2832.7,
      "duration": 2.0,
      "dialogue_text": "A child, then…"
    },
    {
      "subtitle_index": 53,
      "start_time": 2836.575,
      "end_time": 2838.075,
      "duration": 1.5,
      "dialogue_text": "an abandoned child."
    },
    {
      "subtitle_index": 54,
      "start_time": 2849.075,
      "end_time": 2850.325,
      "duration": 1.25,
      "dialogue_text": "It’s on the house."
    },
    {
      "subtitle_index": 55,
      "start_time": 2852.575,
      "end_time": 2853.825,
      "duration": 1.25,
      "dialogue_text": "Thank you, Celeste."
    },
    {
      "subtitle_index": 56,
      "start_time": 2864.45,
      "end_time": 2869.325,
      "duration": 4.875,
      "dialogue_text": "Have you never wondered what it would be like? To be someone else?"
    },
    {
      "subtitle_index": 57,
      "start_time": 2872.575,
      "end_time": 2876.2,
      "duration": 3.625,
      "dialogue_text": "Her? For example?"
    },
    {
      "subtitle_index": 58,
      "start_time": 2878.325,
      "end_time": 2881.575,
      "duration": 3.25,
      "dialogue_text": "Please... stop!"
    },
    {
      "subtitle_index": 59,
      "start_time": 2886.825,
      "end_time": 2887.95,
      "duration": 1.125,
      "dialogue_text": "No?"
    },
    {
      "subtitle_index": 60,
      "start_time": 2892.95,
      "end_time": 2895.575,
      "duration": 2.625,
      "dialogue_text": "Then perhaps the lovely Celeste?"
    },
    {
      "subtitle_index": 61,
      "start_time": 2901.825,
      "end_time": 2902.95,
      "duration": 1.125,
      "dialogue_text": "So it’s her…"
    },
    {
      "subtitle_index": 62,
      "start_time": 2909.7,
      "end_time": 2910.95,
      "duration": 1.25,
      "dialogue_text": "I thought so."
    },
    {
      "subtitle_index": 63,
      "start_time": 2988.075,
      "end_time": 2989.325,
      "duration": 1.25,
      "dialogue_text": "Marie..."
    },
    {
      "subtitle_index": 64,
      "start_time": 3236.95,
      "end_time": 3238.075,
      "duration": 1.125,
      "dialogue_text": "How is the girl?"
    },
    {
      "subtitle_index": 65,
      "start_time": 3238.825,
      "end_time": 3241.325,
      "duration": 2.5,
      "dialogue_text": "Everything is well, sir."
    },
    {
      "subtitle_index": 66,
      "start_time": 3242.45,
      "end_time": 3245.2,
      "duration": 2.75,
      "dialogue_text": "She sleeps soundly… Her appetite is good…"
    },
    {
      "subtitle_index": 67,
      "start_time": 3266.825,
      "end_time": 3268.575,
      "duration": 1.75,
      "dialogue_text": "Everything is as it should be."
    },
    {
      "subtitle_index": 68,
      "start_time": 3299.325,
      "end_time": 3300.825,
      "duration": 1.5,
      "dialogue_text": "Shhhhh…"
    },
    {
      "subtitle_index": 69,
      "start_time": 3303.7,
      "end_time": 3306.2,
      "duration": 2.5,
      "dialogue_text": "I know, I know."
    },
    {
      "subtitle_index": 70,
      "start_time": 3312.325,
      "end_time": 3313.45,
      "duration": 1.125,
      "dialogue_text": "I’m here."
    },
    {
      "subtitle_index": 71,
      "start_time": 3317.2,
      "end_time": 3318.45,
      "duration": 1.25,
      "dialogue_text": "Who… are… you?"
    },
    {
      "subtitle_index": 72,
      "start_time": 3318.7,
      "end_time": 3323.45,
      "duration": 4.75,
      "dialogue_text": "Shhhhh… Don’t try to talk."
    },
    {
      "subtitle_index": 73,
      "start_time": 3328.7,
      "end_time": 3329.95,
      "duration": 1.25,
      "dialogue_text": "Go back to sleep."
    },
    {
      "subtitle_index": 74,
      "start_time": 3334.825,
      "end_time": 3338.825,
      "duration": 4.0,
      "dialogue_text": "My name is Laurence. I’m going to take care of everything."
    },
    {
      "subtitle_index": 75,
      "start_time": 3350.45,
      "end_time": 3356.45,
      "duration": 6.0,
      "dialogue_text": "I want her moved to a private room. I will cover the costs."
    },
    {
      "subtitle_index": 76,
      "start_time": 3358.2,
      "end_time": 3361.45,
      "duration": 3.25,
      "dialogue_text": "What matters above all is that she receives the very best care."
    },
    {
      "subtitle_index": 77,
      "start_time": 3386.575,
      "end_time": 3387.7,
      "duration": 1.125,
      "dialogue_text": "If I might…"
    },
    {
      "subtitle_index": 78,
      "start_time": 3424.45,
      "end_time": 3428.7,
      "duration": 4.25,
      "dialogue_text": "Come on, Celeste… You need to get well."
    },
    {
      "subtitle_index": 79,
      "start_time": 3452.45,
      "end_time": 3453.575,
      "duration": 1.125,
      "dialogue_text": "Good, good, good…"
    },
    {
      "subtitle_index": 80,
      "start_time": 3835.075,
      "end_time": 3837.075,
      "duration": 2.0,
      "dialogue_text": "You will find these satisfactory."
    },
    {
      "subtitle_index": 81,
      "start_time": 4048.075,
      "end_time": 4052.7,
      "duration": 4.625,
      "dialogue_text": "We need your help… with the teeth."
    },
    {
      "subtitle_index": 82,
      "start_time": 4055.325,
      "end_time": 4057.95,
      "duration": 2.625,
      "dialogue_text": "You must ask the masters for help."
    },
    {
      "subtitle_index": 83,
      "start_time": 4059.325,
      "end_time": 4060.45,
      "duration": 1.125,
      "dialogue_text": "Masters?"
    },
    {
      "subtitle_index": 84,
      "start_time": 4063.575,
      "end_time": 4065.45,
      "duration": 1.875,
      "dialogue_text": "Yes, our masters!"
    },
    {
      "subtitle_index": 85,
      "start_time": 4091.95,
      "end_time": 4093.95,
      "duration": 2.0,
      "dialogue_text": "I have to leave soon…"
    },
    {
      "subtitle_index": 86,
      "start_time": 4097.95,
      "end_time": 4099.075,
      "duration": 1.125,
      "dialogue_text": "on the 6th."
    },
    {
      "subtitle_index": 87,
      "start_time": 4104.325,
      "end_time": 4107.2,
      "duration": 2.875,
      "dialogue_text": "The house has been empty since my mother died…"
    },
    {
      "subtitle_index": 88,
      "start_time": 4115.825,
      "end_time": 4117.575,
      "duration": 1.75,
      "dialogue_text": "It’s too big for one person…"
    },
    {
      "subtitle_index": 89,
      "start_time": 4124.2,
      "end_time": 4126.825,
      "duration": 2.625,
      "dialogue_text": "The gardens will be lovely in winter."
    },
    {
      "subtitle_index": 90,
      "start_time": 4138.575,
      "end_time": 4141.575,
      "duration": 3.0,
      "dialogue_text": "I’ll be honoured if you would agree to come with me…"
    },
    {
      "subtitle_index": 91,
      "start_time": 4145.325,
      "end_time": 4146.45,
      "duration": 1.125,
      "dialogue_text": "as my guest."
    },
    {
      "subtitle_index": 92,
      "start_time": 4150.95,
      "end_time": 4152.575,
      "duration": 1.625,
      "dialogue_text": "For as long as you need."
    },
    {
      "subtitle_index": 93,
      "start_time": 4156.825,
      "end_time": 4159.2,
      "duration": 2.375,
      "dialogue_text": "Is there really anything keeping you here?"
    },
    {
      "subtitle_index": 94,
      "start_time": 4362.2,
      "end_time": 4363.575,
      "duration": 1.375,
      "dialogue_text": "Mister Scellinc?"
    },
    {
      "subtitle_index": 95,
      "start_time": 4364.2,
      "end_time": 4365.95,
      "duration": 1.75,
      "dialogue_text": "Have you informed our masters?"
    },
    {
      "subtitle_index": 96,
      "start_time": 4370.95,
      "end_time": 4374.7,
      "duration": 3.75,
      "dialogue_text": "Have you informed our masters that I need assistance?"
    },
    {
      "subtitle_index": 97,
      "start_time": 4374.825,
      "end_time": 4376.075,
      "duration": 1.25,
      "dialogue_text": "I don't understand."
    },
    {
      "subtitle_index": 98,
      "start_time": 4376.325,
      "end_time": 4380.075,
      "duration": 3.75,
      "dialogue_text": "Have you informed our masters that I need assistance?"
    },
    {
      "subtitle_index": 99,
      "start_time": 4403.45,
      "end_time": 4404.7,
      "duration": 1.25,
      "dialogue_text": "What is the matter?"
    },
    {
      "subtitle_index": 100,
      "start_time": 4406.075,
      "end_time": 4407.825,
      "duration": 1.75,
      "dialogue_text": "Dirty sneaking bitch!"
    },
    {
      "subtitle_index": 101,
      "start_time": 4408.741,
      "end_time": 4410.95,
      "duration": 2.2089999999998327,
      "dialogue_text": "- My God - What are you doing?"
    },
    {
      "subtitle_index": 102,
      "start_time": 4583.575,
      "end_time": 4584.7,
      "duration": 1.125,
      "dialogue_text": "I will…"
    },
    {
      "subtitle_index": 103,
      "start_time": 4589.325,
      "end_time": 4590.7,
      "duration": 1.375,
      "dialogue_text": "I will go with you…"
    },
    {
      "subtitle_index": 104,
      "start_time": 4665.45,
      "end_time": 4667.075,
      "duration": 1.625,
      "dialogue_text": "Do you not recognize me?"
    },
    {
      "subtitle_index": 105,
      "start_time": 4669.825,
      "end_time": 4671.2,
      "duration": 1.375,
      "dialogue_text": "I know who you are."
    },
    {
      "subtitle_index": 106,
      "start_time": 4715.325,
      "end_time": 4716.95,
      "duration": 1.625,
      "dialogue_text": "Go and fetch the girl."
    },
    {
      "subtitle_index": 107,
      "start_time": 4760.575,
      "end_time": 4761.7,
      "duration": 1.125,
      "dialogue_text": "Come."
    },
    {
      "subtitle_index": 108,
      "start_time": 4763.325,
      "end_time": 4764.45,
      "duration": 1.125,
      "dialogue_text": "Mia…"
    },
    {
      "subtitle_index": 109,
      "start_time": 4770.95,
      "end_time": 4772.45,
      "duration": 1.5,
      "dialogue_text": "Put that thing away."
    },
    {
      "subtitle_index": 110,
      "start_time": 4805.825,
      "end_time": 4807.075,
      "duration": 1.25,
      "dialogue_text": "On the table."
    },
    {
      "subtitle_index": 111,
      "start_time": 4811.95,
      "end_time": 4813.575,
      "duration": 1.625,
      "dialogue_text": "Lie down on the table."
    },
    {
      "subtitle_index": 112,
      "start_time": 4859.7,
      "end_time": 4861.325,
      "duration": 1.625,
      "dialogue_text": "This will take some time."
    },
    {
      "subtitle_index": 113,
      "start_time": 4866.7,
      "end_time": 4868.2,
      "duration": 1.5,
      "dialogue_text": "Please leave us alone."
    },
    {
      "subtitle_index": 114,
      "start_time": 4900.575,
      "end_time": 4901.825,
      "duration": 1.25,
      "dialogue_text": "Open…"
    },
    {
      "subtitle_index": 115,
      "start_time": 4905.2,
      "end_time": 4906.325,
      "duration": 1.125,
      "dialogue_text": "Wider."
    },
    {
      "subtitle_index": 116,
      "start_time": 5009.45,
      "end_time": 5014.2,
      "duration": 4.75,
      "dialogue_text": "It’s a different system now. The glass implants are permanent."
    },
    {
      "subtitle_index": 117,
      "start_time": 5015.95,
      "end_time": 5019.95,
      "duration": 4.0,
      "dialogue_text": "You must attend scrupulously to the girl’s hygiene in the days to come."
    },
    {
      "subtitle_index": 118,
      "start_time": 5022.95,
      "end_time": 5025.825,
      "duration": 2.875,
      "dialogue_text": "Saltwater will keep her mouth free of infection."
    },
    {
      "subtitle_index": 119,
      "start_time": 5032.45,
      "end_time": 5033.7,
      "duration": 1.25,
      "dialogue_text": "If you can manage that,"
    },
    {
      "subtitle_index": 120,
      "start_time": 5033.95,
      "end_time": 5037.325,
      "duration": 3.375,
      "dialogue_text": "there will be no need for me to return before your departure."
    },
    {
      "subtitle_index": 121,
      "start_time": 5357.825,
      "end_time": 5359.075,
      "duration": 1.25,
      "dialogue_text": "Not long now."
    },
    {
      "subtitle_index": 122,
      "start_time": 5363.325,
      "end_time": 5364.45,
      "duration": 1.125,
      "dialogue_text": "It hurts."
    },
    {
      "subtitle_index": 123,
      "start_time": 5367.575,
      "end_time": 5368.825,
      "duration": 1.25,
      "dialogue_text": "Laurence, please."
    },
    {
      "subtitle_index": 124,
      "start_time": 5411.95,
      "end_time": 5416.075,
      "duration": 4.125,
      "dialogue_text": "I am sure that in our life together you will no longer feel such need of it…"
    },
    {
      "subtitle_index": 125,
      "start_time": 5550.075,
      "end_time": 5551.575,
      "duration": 1.5,
      "dialogue_text": "May I help you madam?"
    },
    {
      "subtitle_index": 126,
      "start_time": 5800.575,
      "end_time": 5801.95,
      "duration": 1.375,
      "dialogue_text": "It’s on the house."
    },
    {
      "subtitle_index": 127,
      "start_time": 6296.325,
      "end_time": 6300.325,
      "duration": 4.0,
      "dialogue_text": "I have brought the child, as I was asked."
    },
    {
      "subtitle_index": 128,
      "start_time": 6302.45,
      "end_time": 6303.95,
      "duration": 1.5,
      "dialogue_text": "I am Albert Scellinc."
    },
    {
      "subtitle_index": 129,
      "start_time": 6318.45,
      "end_time": 6320.075,
      "duration": 1.625,
      "dialogue_text": "Please wait in the hall."
    },
    {
      "subtitle_index": 130,
      "start_time": 6361.575,
      "end_time": 6363.575,
      "duration": 2.0,
      "dialogue_text": "She has been well cared for."
    },
    {
      "subtitle_index": 131,
      "start_time": 6366.45,
      "end_time": 6367.575,
      "duration": 1.125,
      "dialogue_text": "So I can see."
    },
    {
      "subtitle_index": 132,
      "start_time": 6376.825,
      "end_time": 6379.45,
      "duration": 2.625,
      "dialogue_text": "Everything is as it should be."
    },
    {
      "subtitle_index": 133,
      "start_time": 6382.075,
      "end_time": 6383.7,
      "duration": 1.625,
      "dialogue_text": "You mustn’t be afraid."
    },
    {
      "subtitle_index": 134,
      "start_time": 6394.95,
      "end_time": 6396.075,
      "duration": 1.125,
      "dialogue_text": "Perfect!"
    },
    {
      "subtitle_index": 135,
      "start_time": 6397.95,
      "end_time": 6402.2,
      "duration": 4.25,
      "dialogue_text": "No doubt you have papers for me to sign."
    },
    {
      "subtitle_index": 136,
      "start_time": 6403.325,
      "end_time": 6406.7,
      "duration": 3.375,
      "dialogue_text": "They are waiting for you at reception."
    },
    {
      "subtitle_index": 137,
      "start_time": 6415.45,
      "end_time": 6417.7,
      "duration": 2.25,
      "dialogue_text": "Will you be leaving anything with her?"
    },
    {
      "subtitle_index": 138,
      "start_time": 6421.825,
      "end_time": 6425.7,
      "duration": 3.875,
      "dialogue_text": "Your daughter. Will you be leaving anything with her?"
    },
    {
      "subtitle_index": 139,
      "start_time": 6425.825,
      "end_time": 6427.45,
      "duration": 1.625,
      "dialogue_text": "She is not my daughter!"
    },
    {
      "subtitle_index": 140,
      "start_time": 6428.825,
      "end_time": 6430.45,
      "duration": 1.625,
      "dialogue_text": "But you’re signing for her?"
    },
    {
      "subtitle_index": 141,
      "start_time": 6431.2,
      "end_time": 6432.7,
      "duration": 1.5,
      "dialogue_text": "I am just her keeper."
    },
    {
      "subtitle_index": 142,
      "start_time": 6437.575,
      "end_time": 6438.7,
      "duration": 1.125,
      "dialogue_text": "Look!"
    },
    {
      "subtitle_index": 143,
      "start_time": 6453.45,
      "end_time": 6454.575,
      "duration": 1.125,
      "dialogue_text": "It’s a lie!"
    },
    {
      "subtitle_index": 144,
      "start_time": 6597.825,
      "end_time": 6598.908,
      "duration": 1.0830000000005384,
      "dialogue_text": "Marie?"
    }
  ]
}